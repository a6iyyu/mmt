{
  "name": "mmt",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "prisma generate && next build",
    "db:push": "dotenv -e .env.local -- bunx prisma db push",
    "db:reset": "dotenv -e .env.local -- bunx prisma migrate reset --force",
    "dev": "dotenv -e .env.local -- next dev",
    "format": "prettier --write . --ext .js,.jsx,.ts,.tsx,.json,.md",
    "knip": "knip . --tsconfig ./tsconfig.json",
    "lint": "eslint",
    "start": "next start",
    "seed": "bun prisma/seed.ts",
    "test:e2e": "playwright test --project=chromium",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
